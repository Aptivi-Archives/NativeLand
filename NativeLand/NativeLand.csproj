<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Aptivi, Oleg Tarasov</Authors>
    <Description>A .NET Standard 2.0 library to manage native dependencies.</Description>
    <PackageProjectUrl>https://github.com/Aptivi/NativeLand</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Aptivi/NativeLand</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\NativeLand.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="3.1.4" />
  </ItemGroup>

</Project>
